<Window x:Class="ManualRamosAddon.AddDpDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:cmd="clr-namespace:GalaSoft.MvvmLight.Command;assembly=GalaSoft.MvvmLight.Platform"
        xmlns:local="clr-namespace:ManualRamosAddon"
        xmlns:thermoWigets="clr-namespace:ThermoWpfWidgets;assembly=ThermoWpfWidgets"        
        mc:Ignorable="d"
        Title="Select Tag" Height="300" Width="300"
        DataContext="{Binding AddDpDialog, Source={StaticResource Locator}}">
    <Grid>
        <thermoWigets:ThermoDPTreeWidget HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                         MouseDoubleClickSelectionEvent="ThermoDPTreeWidget_MouseDoubleClickSelectionEvent"/>    
    </Grid>
</Window>
