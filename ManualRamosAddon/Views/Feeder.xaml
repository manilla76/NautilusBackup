<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ignore="http://www.galasoft.ch/ignore"
             xmlns:thermo="clr-namespace:ThermoWpfWidgets;assembly=ThermoWpfWidgets"
        xmlns:ThermoWpfConverters="clr-namespace:ThermoWpfConverters;assembly=ThermoWpfConverters" x:Class="ManualRamosAddon.Feeder"
        mc:Ignorable="d ignore"
        >
    <UserControl.DataContext>
        <Binding Path="Feeder" Source="{StaticResource Locator}"/>
    </UserControl.DataContext>

    <Grid>

        <StackPanel>
            <TextBlock Text="{Binding Oxide}"/>
            <TextBlock Text="{Binding IsManual}"/>
            
        </StackPanel>

    </Grid>
</UserControl>
